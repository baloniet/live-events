// Place your settings in this file to overwrite default and user settings.
{ 
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/*.js": { "when": "$(basename).ts"},
        "**/*.js.map": { "when": "$(basename)"},
        "**/*.spec.ts": true,
        "**/*.metadata.json": true,
        "node_modules": true,
        "typings": true,
        "e2se": true
    },
    "files.autoSave": "onFocusChange"
}